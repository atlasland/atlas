{
  "fmt": {
    "files": {
      "exclude": [
        ".git",
        ".deno",
        "cov"
      ]
    }
  },
  "lint": {
    "files": {
      "exclude": [
        ".git",
        ".deno",
        "cov"
      ]
    }
  },
  "tasks": {
    "cache": "deno cache --reload --lock=lock.json --lock-write cli.ts log/mod.ts console/mod.ts",
    "check": "deno check cli.ts log/mod.ts console/mod.ts",
    "test": "deno test --unstable --allow-all --coverage=./cov"
  }
}
